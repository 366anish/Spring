<persistence xmlns="https://java.sun.com/xml/ns/persistence"
			 xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="https://java.sun.com/xml/ns/persistence
          https://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
			 version="2.0">

	<persistence-unit name="NoExcludeElement" />

	<persistence-unit name="EmptyExcludeElement">
		<exclude-unlisted-classes />
	</persistence-unit>

	<persistence-unit name="TrueExcludeElement">
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
	</persistence-unit>

	<persistence-unit name="FalseExcludeElement">
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
	</persistence-unit>
</persistence>
